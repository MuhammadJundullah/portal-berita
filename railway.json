{
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@railway/vite",
            "config": {
                "buildCommand": "npm run build",
                "outputDir": "public/build"
            }
        },
        {
            "src": "package.json",
            "use": "npm",
            "cmd": "npm run build"
        }
    ],
    "deploy": {
        "startCommand": "php artisan serve --port=$PORT"
    }
}
